value strEvals = OutputStrings(id=${resultsId}, storage="MIDString", collection="ExperimentEvaluationOutput", 
                 refs=
                 (
                  ${eval.devAccuracy}, 
                  ${eval.devPrecision}, 
                  ${eval.devRecall}, 
                  ${eval.devF},
